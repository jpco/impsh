[vars]
~home=/home/jpco
home=/home/jpco
__jpsh_debug=1
__jpsh_color=1

[env]
HOME=/home/jpco
SHELL=/home/jpco/bin/jpsh
LS_COLORS=rs=0:di=01;34:ln=01;36:mh=00:pi=40;33:so=01;35:do=01;35:bd=40;33;01:cd=40;33;01:or=40;31;01:su=37;41:sg=30;43:ca=30;41:tw=30;42:ow=34;42:st=37;44:ex=01;32

[alias]
ls=ls --color=auto
rm=/usr/bin/rmpp
jpco.io=ssh jpco.io -t tmux a
uzbl=uzbl-tabbed
kb=sudo keybr

[startup]
echo Hello\ (SHELL)!
